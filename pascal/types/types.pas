  writeln('i: integer; // = ', i);
  writeln('shi: shortint; // = ', shi);
  writeln('smi: smallint; // = ', smi);
  writeln('loi: longint; // = ', loi);
  writeln('i64: int64; // = ', i64);
  writeln('bt: byte; // = ', bt);
  writeln('w: word; // = ', w);
  writeln('low: longword; // = ', low);
end.

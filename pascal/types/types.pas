program Types;

var b: boolean;
var ca: cardinal;
var i: integer;
var shi: shortint;
var smi: smallint;
var loi: longint;
var i64: int64;
var bt: byte;
var w: word;
var low: longword;

begin
  writeln('boolean b: ', b);
  writeln('cardinal ca: ', ca);
  writeln('integer i: ', i);
  writeln('shortint shi: ', shi);
  writeln('smallint smi: ', smi);
  writeln('longint loi: ', loi);
  writeln('int64 i64: ', i64);
  writeln('byte bt: ', bt);
  writeln('word w: ', w);
  writeln('longword low: ', low);
end.
